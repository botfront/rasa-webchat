    "react-immutable-proptypes": "^2.2.0",
    "react-inlinesvg": "^2.3.0",
    "react-markdown": "^4.3.1",
    "react-popper": "^2.2.5",
    "react-redux": "^7.2.6",
    "react-slick": "^0.28.1",
    "react-textarea-autosize": "^8.3.3",
    "redux": "^4.1.2",
    "slick-carousel": "^1.8.1",
    "socket.io-client": "^3.1.2",
    "sockjs-client": "^1.4.0"
