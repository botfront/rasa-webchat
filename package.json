{
  "name": "rasa-webchat",
  "version": "1.0.2",
  "description": "Chat web widget for React apps and Rasa Core chatbots",
  "module": "module/index.js",
  "main": "lib/index.js",
  "repository": "git@https://github.com/botfront/rasa-webchat.git",
  "author": "",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js -p",
    "test": "jest",
    "prepare": "npm run build",
    "release": "standard-version"
  },
  "keywords": [
    "react",
    "chat",
    "widget",
    "javascript"
  ],
  "dependencies": {
    "@popperjs/core": "^2.4.0",
    "@stomp/stompjs": "^5.4.2",
    "html-webpack-plugin": "^3.2.0",
    "immutable": "^3.8.2",
    "object-hash": "^1.1.5",
    "prop-types": "^15.7.2",
    "react-immutable-proptypes": "^2.2.0",
    "react-markdown": "^8.0.2",
    "react-popper": "^2.3.0",
    "react-redux": "^7.1.3",
    "react-slick": "^0.29.0",
    "react-textarea-autosize": "^8.5.3",
    "redux": "^4.0.5",
    "slick-carousel": "^1.8.1",
    "socket.io": "^3.1.2",
    "socket.io-client": "^3.1.2",
    "sockjs-client": "^1.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^1.6.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^3.0.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^23.2.0",
    "lodash-webpack-plugin": "^0.11.5",
    "prettier": "^1.18.2",
    "prettier-eslint": "^5.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.69.7",
    "sass-loader": "^7.3.1",
    "standard-version": "^9.0.0",
    "string-replace-loader": "^2.3.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "verbose": true,
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(react-markdown)/)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|less|scss|sass)$": "<rootDir>/mocks/styleMock.js",
      "react-markdown": "<rootDir>/node_modules/react-markdown/react-markdown.min.js"
    },
    "setupFilesAfterEnv": ["<rootDir>/test-setup.js"]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
