    "eslint-plugin-react": "^7.29.4",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^7.0.4",
    "jest": "^25.5.4",
    "lodash-webpack-plugin": "^0.11.6",
    "prettier": "2.6.0",
    "prettier-eslint": "13.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.49.9",
    "sass-loader": "10.2.0",
    "standard-version": "^9.3.2",
    "string-replace-loader": "^2.3.0",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
